	public void ReadTheFile()
	   {
		  try{//C:\\Users\\paula.pc\\Desktop\\lzw\\lzw\\
	          x =new Scanner(new File("compresed data.txt"));
	         }catch(Exception e)
	         {
	               System.out.println("Couldn't find the file!");
	         }
	  
  			//reading from the file and save is string
			  while(x.hasNext()){
